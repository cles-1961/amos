{
  "name": "data-grouper",
  "version": "1.1.0",
  "main": "app/main.js",
  "description": "資料分組",
  "author": "codfish",
  "scripts": {
    "start": "vite build --mode electron && npm run electron:start",
    "web:dev": "vite --host",
    "dev": "concurrently -k \"vite\" \"npm run electron:dev\"",
    "build": "vue-tsc --noEmit --skipLibCheck && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "electron:start": "electron-forge start",
    "electron:dev": "cross-env IS_DEV=true electron-forge start",
    "electron:build": "vite build --mode electron && electron-forge make",
    "electron:package": "vite build --mode electron && electron-forge package"
  },
  "dependencies": {
    "@quasar/extras": "^1.15.3",
    "@vueuse/core": "^9.0.0",
    "await-to-js": "^3.0.0",
    "decimal.js": "^10.4.3",
    "electron-squirrel-startup": "^1.0.0",
    "exceljs": "^4.3.0",
    "file-saver": "^2.0.5",
    "lodash-es": "^4.17.21",
    "nanoid": "^4.0.0",
    "pinia": "^2.0.12",
    "quasar": "^2.8.2",
    "vite-plugin-package-version": "^1.0.2",
    "vue": "^3.2.16",
    "vue-router": "^4.0.14",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.4",
    "@electron-forge/maker-deb": "^6.0.4",
    "@electron-forge/maker-rpm": "^6.0.4",
    "@electron-forge/maker-squirrel": "^6.0.4",
    "@electron-forge/maker-zip": "^6.0.4",
    "@pinia/testing": "^0.0.14",
    "@quasar/vite-plugin": "^1.2.0",
    "@types/file-saver": "^2.0.5",
    "@types/lodash-es": "^4.17.6",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vitejs/plugin-legacy": "^2.3.1",
    "@vitejs/plugin-vue": "^1.10.2",
    "@vitest/coverage-c8": "^0.25.3",
    "@vitest/ui": "^0.25.3",
    "@vue/test-utils": "^2.2.4",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "electron": "22.0.0",
    "esbuild": "^0.16.1",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^8.7.1",
    "happy-dom": "^7.7.0",
    "postcss": "^8.4.12",
    "prettier": "2.6.2",
    "sass": "1.32.0",
    "tailwindcss": "^3.0.23",
    "terser": "^5.16.1",
    "type-fest": "^2.19.0",
    "typescript": "~4.7.0",
    "vite": "^3.2.4",
    "vitest": "^0.25.3",
    "vue-tsc": "^1.0.9"
  }
}